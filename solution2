Code for Question 2
#include <stdio.h>

int main(){
    int arr[10]={6,2,5,5,4,5,6,3,7,6};
    int n;
    int res[2];
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        int a,b,c,x;
        int num_sticks=0;
        scanf("%d %d %d %d",&a,&b,&c,&x);
        int ans = a*x+b*x+c;
        while(ans>0){
            int temp=ans%10;
            num_sticks=num_sticks+arr[temp];
            ans=ans/10;
        }
        res[i]=num_sticks;
    }

    for(int i=0;i<n;i++){
        printf("%d\n",res[i]);
    }
    return 0;
}
